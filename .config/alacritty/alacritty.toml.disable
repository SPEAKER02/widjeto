[window]

padding = { x = 15, y = 15 }

opacity = 0.99

blur = false

[font]

normal = { family = "Agave Nerd Font", style = "Regular" }
size = 10.00

[colors]
	[colors.primary]

	foreground = "#CAD3F5"
	background = "#24273A"
	[colors.normal]
	
	black = "#494D64"
	red = "#ED8796"
	green = "#A6DA95"
	yellow = "#EED49F"
	blue = "#8AADF4"
	magenta = "#F5BDE6"
	cyan = "#8BD5CA"
	white = "#B8C0E0"
	[colors.bright]

	black = "#5B6078"
        red = "#ED8796"
        green = "#A6DA95"
        yellow = "#EED49F"
        blue = "#8AADF4"
        magenta = "#F5BDE6"
        cyan = "#8BD5CA"
        white = "#A5ADCB"

[cursor]

style = { shape = "Block", blinking = "On" }
